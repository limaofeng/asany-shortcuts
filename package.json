{
  "version": "0.1.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@asany/shortcuts",
  "author": "李茂峰",
  "module": "dist/shortcuts.esm.js",
  "repository": "https://github.com/limaofeng/asany-shortcuts",
  "size-limit": [
    {
      "path": "dist/shortcuts.cjs.production.min.js",
      "limit": "500 KB",
      "ignore": [
        "fonteditor-core"
      ]
    },
    {
      "path": "dist/shortcuts.esm.js",
      "limit": "200 KB",
      "ignore": [
        "fonteditor-core"
      ]
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@size-limit/preset-small-lib": "^7.0.3",
    "@types/invariant": "^2.2.35",
    "@types/lodash-es": "^4.17.5",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.0",
    "conventional-changelog-cli": "^2.1.1",
    "cssnano": "^5.0.11",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "pinst": "^2.1.6",
    "postcss": "^8.4.1",
    "postcss-modules": "^4.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "combokeys": "^3.0.1",
    "invariant": "^2.2.4",
    "just-reduce-object": "^2.0.1",
    "lodash": "^4.17.21",
    "platform": "^1.3.6"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/test/globals-test.ts"
    ]
  }
}
